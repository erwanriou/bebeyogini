{
  "name": "bebeyogini",
  "version": "1.1.0",
  "description": "Website for Janis, blog, biowebsite and probably new fonctionalities",
  "main": "server.js",
  "engines": {
    "node": "13.2.0",
    "npm": "6.13.1"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "pwa",
    "react",
    "redux",
    "express",
    "node",
    "architects",
    "archsplace",
    "mongoDB"
  ],
  "author": "Erwan Riou <rr.1.riou@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.343.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "concurrently": "^3.6.1",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "gravatar": "^1.6.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.8.3",
    "mongoose-unique-validator": "^2.0.2",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-instagram": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^3.3.2",
    "validator": "^10.6.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erwanriou/bebeyogini"
  }
